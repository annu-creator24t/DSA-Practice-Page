------------------code 1------------------ 

// Interface
// An interface in a java is an Abstract type that defines a set of methods a class must implements
// Multiple interfaces using interfaces

interface tv {
    void watchingTv();
}

interface food {
    void eatingFood();
}

class Student implements tv, food {
    public void watchingTv() {
        System.out.println("watchingTv");
    }

    public void eatingFood() {
        System.out.println("eatingFood");
    }
}

public class Main {
    public static void main(String[] args) {
        Student s = new Student();
        s.watchingTv();
        s.eatingFood();
    }
}


------------------code 2 ---------------------
// Super Keyword

class Person {
    // Runs first when CollegeStudent object is created because of inheritance (via super)
    Person(String name) {
        System.out.println("Person Name: " + name);
    }
}

// Child class inheriting Person
class CollegeStudent extends Person {

    // Constructor of CollegeStudent
    CollegeStudent(String name) {

        // Calls the parent (Person) constructor first
        // super() must be the first line in a child constructor
        super(name);

        System.out.println("College student constructor called");
    }
}

public class Main {

    public static void main(String[] args) {
        CollegeStudent cs = new CollegeStudent("Annu");
    }
}


 ------------------Code 3----------------------
// Inheriting Attributes 

class University{
    String UniversityName = "IILM University";
}


class College extends University{
    void displayUniversityName(){
        System.out.println("University : " + UniversityName);
    }
}
 
public class Main
{
	public static void main(String[] args) {
		College c = new College();
		c.displayUniversityName();
	}
}


-------------code 4--------------
//Inheriting methods 

class Parent{
    void pocketMoney(){
        System.out.println("Pocket Money Credit");
    }
}
class child extends Parent{
    void freelancing(){
        System.out.println("khud ka paisa");
    }
}
public class Main
{
	public static void main(String[] args) {
		child ch = new child();
		ch.pocketMoney();
		ch.freelancing();
	}
}
