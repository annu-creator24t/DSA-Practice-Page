code 1- single level inheritance

class Vehicle{
    void start(){
        System.out.println("Vehicle starts");
    }
}
class Bike extends Vehicle{
    void drive(){
        System.out.println("Bike running");
    }
    
}
class Main {
    public static void main(String[] args) {
        Bike b=new Bike();
        b.start();
        b.drive();
    }
}

code 2 - Multi level inheritance

class Vehicle {
    void start() {
        System.out.println("Vehicle starts");
    }
}

class Bike extends Vehicle {
    void drive() {
        System.out.println("Bike running");
    }
}

class SportsBike extends Bike {
    void turbo() {
        System.out.println("Turbo boosted");
    }
}

class Main {
    public static void main(String[] args) {
        SportsBike sb = new SportsBike();
        sb.start(); 
        sb.drive();  
        sb.turbo();   
    }
}


Code 3- Hierachical Inheritance

class Vehicle {
    void start() {
        System.out.println("Vehicle starting");
    }
}

class Bike extends Vehicle {
    void drink() {
        System.out.println("Bike running");
    }
}

class Car extends Vehicle {
    void drive() {
        System.out.println("Car Running");
    }
}

public class Main {
    public static void main(String[] args) {
        Bike b = new Bike();
        Car c = new Car();

        b.start();
        b.drink();

        c.start();
        c.drive();

    }
}

